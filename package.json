  "homepage": "https://github.com/hbhde/markable#readme",
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "husky": "^4.2.3",
    "mocha": "^7.1.1",
    "nyc": "^15.0.1"
  }
